{
  "name": "@rweich/semantic-release-config",
  "version": "2.0.6",
  "description": "semantic-release config for my projects",
  "author": "rweich",
  "license": "MIT",
  "main": ".releaserc.json",
  "scripts": {
    "prepare": "husky install",
    "test-release": "semantic-release --dry-run --debug --branches main"
  },
  "peerDependencies": {
    "semantic-release": "^22.0.5"
  },
  "dependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "conventional-changelog-conventionalcommits": "^7.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "husky": "^8.0.3",
    "semantic-release": "^22.0.5"
  },
  "engines": {
    "node": ">=18.17"
  },
  "repository": "rweich/semantic-release-config",
  "files": [
    "README.md",
    ".releaserc.json"
  ],
  "keywords": [
    "semantic-release",
    "semantic-release-config"
  ],
  "publishConfig": {
    "access": "public"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
